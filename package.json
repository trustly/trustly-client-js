{
  "name": "@trustly-group/client",
  "version": "0.0.1",
  "type": "commonjs",
  "description": "Client for Trustly API",
  "bugs": {
    "url": "https://github.com/trustly/trustly-client-js/issues"
  },
  "homepage": "https://github.com/trustly/trustly-client-js#readme",
  "keywords": [
    "trustly",
    "api"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trustly/trustly-client-js.git"
  },
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && npm run bundle && npm run cleanup",
    "bundle": "rollup -c",
    "cleanup": "mv dist/index.d.ts dist/index.tmp && find ./dist -name '*.d.ts' -type f -delete && mv dist/index.tmp dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "README.md"
  ],
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-inline-import": "^3.0.0",
    "eslint": "^8.56.0",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.21.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-string": "^3.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "type-fest": "^4.26.0",
    "typeconv": "^2.3.1",
    "typescript": "^5.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "testEnvironment": "node"
  }
