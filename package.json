{
  "name": "trustly-client-js",
  "packageManager": "pnpm@8.7.4",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "tsc && npm run bundle",
    "bundle": "rollup -c",
    "postbundle": "mv dist/index.d.ts dist/index.tmp && find ./dist -name '*.d.ts' -type f -delete && mv dist/index.tmp dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@types/uuid": "^9.0.1",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "rollup": "^3.29.1",
    "rollup-plugin-dts": "^6.0.2",
    "rimraf": "^5.0.1",
    "shx": "^0.3.4",

    "babel-loader": "^9.1.3",

    "babel-jest": "^29.6.4",
    "babel-plugin-inline-import": "^3.0.0",
    "eslint": "^8.49.0",
    "jest": "^29.6.4",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2",
    "express": "^4.18.2"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "testEnvironment": "node"
  }
}
